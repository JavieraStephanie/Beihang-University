package elevator;

import java.util.ArrayList;

public class Queue {
	private ArrayList<Request> queue = new ArrayList<Request>();;
	
	public boolean addQueue (Request req) {
		queue.add(req);
		return true;
	}
	
	public boolean removeQueue (int i) {
		queue.remove(i);
		return true;
	}
	
	public Request getQueue (int i) {
		return queue.get(i);
	}
	
	public int getQueueSize () {
		return queue.size();
	}
}
